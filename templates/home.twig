{% extends "layout.twig" %}


{% block content %}

<div class="row">


    <section id="home-banner" class="col-lg-12">
    	<h1>Home</h1>
    </section>
</div>
<div class="row">
	<div class="col-lg-12">
          <div id="carouselExampleControls" class="carousel slide carousel-fade" data-ride="carousel">
          	<div class="carousel-inner">
            <div class="carousel-item active">
              <img src="../uploads/photos/{{ images.0.filename }}" class="d-block carousel-photo" alt="...">
            </div>
            <div class="carousel-item">
              <img src="../uploads/photos/{{ images.1.filename }}" class="d-block carousel-photo" alt="...">
            </div>
            <div class="carousel-item">
              <img src="../uploads/photos/{{ images.2.filename }}" class="d-block carousel-photo" alt="...">
            </div>
          </div>
          <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
          </a>
          <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
          </a>
        </div>
	</div>
</div>
<div class="row">
	<div id="footer" class="col-lg-12"></div>
</div>

